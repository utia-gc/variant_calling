process {
    withLabel: 'base' {
        container = 'biocontainers/biocontainers:v1.2.0_cv2'
    }

    withLabel: 'bwa_mem2' {
        container = 'https://depot.galaxyproject.org/singularity/bwa-mem2%3A2.2.1--hd03093a_5'
    }

    withLabel: 'cutadapt' {
        container = "https://depot.galaxyproject.org/singularity/cutadapt%3A4.4--py39hf95cd2a_1"
    }

    withLabel: 'fastp' {
        container = 'https://depot.galaxyproject.org/singularity/fastp%3A0.23.4--hadf994f_2'
    }

    withLabel: 'fastqc' {
        container = "https://depot.galaxyproject.org/singularity/fastqc%3A0.12.1--hdfd78af_0"
    }

    withLabel: 'gatk' {
        container = 'https://depot.galaxyproject.org/singularity/gatk4%3A4.4.0.0--py36hdfd78af_0'
    }

    withLabel: 'multiqc' {
